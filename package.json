{
  "name": "chatgpt-client",
  "version": "1.1.3",
  "description": "A Desktop client made with electron for ChatGPT",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.evermine.chatgptclient",
    "productName": "ChatGPT - Client",
    "files": [
      "index.js",
      "app",
      "icon.png"
    ],
    "publish": {
      "provider": "github"
    },
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^27.0.1",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "electron-log": "^5.0.0",
    "electron-updater": "^6.1.4",
    "request": "^2.88.2"
  }
}
